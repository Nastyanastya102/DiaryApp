{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "start",
      "args": ["start"]
    },
    {
      "taskName": "storybook",
      "args": ["run", "storybook"]
    },
    {
      "taskName": "test",
      "args": ["run", "test"]
    }
  ]
}
